<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:TodoList_sample_app.ViewModels"
                    xmlns:views="clr-namespace:TodoList_sample_app.Views">
    <DataTemplate DataType="{x:Type viewModels:CalendarVm}">
        <views:CalendarUc />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:DayVm}">
        <views:DayUc />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:ItemVm}">
        <views:ItemUc />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:NotificationsVm}">
        <views:NotificationsUc />
    </DataTemplate>
</ResourceDictionary>